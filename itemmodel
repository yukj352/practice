class Item{
  final int id;
  final String name;
  final String category;
  final double amt;
  final int qty;
  final DateTime date;
  final String description;

  Item({
    required this.id,
   required this.name,
    required this.category,
    required this.amt,
    required this.qty,
    required this.date,
    required this.description,
});
}
